@model TechJobs6Persistent.Models.Skill

<h1>Add New Skill</h1>


<form asp-controller="Skill" asp-action="Add" method="post">

    <div class="form-group">
        <label asp-for="SkillName">Skill Name</label>
        <input asp-for="SkillName" />
        <span asp-validation-for="SkillName"></span>

    </div>

    <div class="form-group">
        <label asp-for="SkillDescription">Skill Description</label>
        <input asp-for="SkillDescription" />
        <span asp-validation-for="SkillDescription"></span>
    </div>

    <input type="submit" value="Submit" />

</form>
